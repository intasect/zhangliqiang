			xformsversion="2"
			key="User Interface/com/laiyifen/test/TestDialog"
			focusatstartup="true">
			<xhead>
				<xml
					xmlns="">
					<xml
						id="Application_SelectTeams">
						<Application>
							<id>selectDepts</id>
							<url>cordys/laiyifen/common/html/selectDepts.htm</url>
							<caption>选择部门</caption>
							<frame
								features="dialogWidth:700px;dialogHeight:450px;center:yes;help:no;titlebar:no;status:no;">_modal</frame>
							<data/>
						</Application>
					</xml>
					<xml
						id="Application_SelectLeader">
						<Application>
							<id>selectUsers</id>
							<description>selectUsers</description>
							<caption>选择用户</caption>
							<url>cordys/laiyifen/common/html/selectUsers.htm</url>
							<frame
								features="dialogWidth:800px;dialogHeight:500px;resizable:yes">_modal</frame>
						</Application>
					</xml>
				</xml>
			</xhead>
			<xbody>
				<xforms:group
					wcpforms:layout="vertical"
					id="xbody_group"
					wcpforms:labelalign="ontop">
					<xforms:group
						appearance="controlbox"
						wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsControlbox"
						id="controlbox1"
						controltype="button"
						wcpforms:layout="horizontal"
						wcpforms:class="v_layout autoextent"
						wcpforms:align="left_align"
						wcpforms:size="null auto">
						<xforms:trigger
							wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsButton"
							id="button1"
							wcpforms:class="h_button"
							wcpforms:size="null 2em">
							<xforms:label
								textIdentifier="000C297E-6FFF-11E1-E391-A1D0D61415D6">SelectDept</xforms:label>
							<xforms:action
								ev:event="xforms-activate">
								<xforms:button1_Click/>
							</xforms:action>
						</xforms:trigger>
						<xforms:trigger
							wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsButton"
							id="button2"
							wcpforms:class="h_button"
							wcpforms:size="null 2em">
							<xforms:label
								textIdentifier="78e3b51c-e998-11e0-faec-469c655b1f1e">SelectUser</xforms:label>
							<xforms:action
								ev:event="xforms-activate">
								<xforms:button2_Click/>
							</xforms:action>
						</xforms:trigger>
					</xforms:group>
					<xforms:textarea
						wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsTextarea"
						id="textarea1"
						doEBIValidate="false"
						wcpforms:layout="vertical"
						wcpforms:class="v_layout"
						wcpforms:size="null 116px">
						<xforms:label
							textIdentifier="000C297E-6FFF-11E1-E391-A1D0D61355D6"
							wcpforms:class="v_label">Selected Result</xforms:label>
					</xforms:textarea>
					<xforms:input
						wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsInput"
						id="input1"
						doEBIValidate="false"
						wcpforms:class="v_layout"
						wcpforms:size="null 32px">
						<xforms:label
							textIdentifier="78e3b51c-e998-11e1-e123-1ea131afd242"
							wcpforms:class="v_label">Input1</xforms:label>
						<wcpforms:zoom>
							<wcpforms:url
								documentID="78e3b51c-e998-11e1-e222-0fc28b1a9f8f">/cordys/User Interface/com/laiyifen/goods/ABC.caf</wcpforms:url>
							<wcpforms:businessAttribute>input1</wcpforms:businessAttribute>
							<xforms:trigger>
								<xforms:label>
									<img
										xmlns="http://schemas.cordys.com/1.0/xform"
										src="/cordys/wcp/theme/default/icon/lookup/lookup_file.png"
									/>
								</xforms:label>
							</xforms:trigger>
						</wcpforms:zoom>
					</xforms:input>
				</xforms:group>
			</xbody>
			<xformsscript>
				<![CDATA[function button1_Click(eventObject)
{
    var oParam = new Object();
    oParam.deptDN = "";//all dept
    oParam.selectMode = "MULTIPLE";
    oParam.callBack = closeHandler;
    application.showDialog(Application_SelectTeams.XMLDocument.documentElement, oParam, null, null, false);
}

function closeHandler(dialogReturnValue)
{
    //The logic to handle the dialogReturnValue goes here.
    textarea1.setValue(dialogReturnValue.selectedDeptList + "===========" +dialogReturnValue.selectedDeptNameList);
}

function button2_Click(eventObject)
{
    var oParam = new Object();
    oParam.selectMode = "MULTIPLE";
    oParam.callBack = closeHandler1;
    application.showDialog(Application_SelectLeader.XMLDocument.documentElement, oParam, null, null, false);
}

function closeHandler1(dialogReturnValue)
{
    //The logic to handle the dialogReturnValue goes here.
    textarea1.setValue(dialogReturnValue.selectedUserList+ "===========" +dialogReturnValue.selectedUserNameList + "===========" +dialogReturnValue.currentSslectedUserCodeList);
}]]>
			</xformsscript>
		</xformhtml>
