					/>
					<wcpforms:instanceschema/>
				</xforms:model>
				<xml
					xmlns="">
					<xml
						id="SOAP_GetStatisticsShopByLifecycleStatus">
						<SOAP:Envelope
							xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/">
							<SOAP:Body>
								<GetStatisticsShopByLifecycleStatus
									xmlns="http://schemas.cordys.com/tbpm/shop"
									preserveSpace="no"
									qAccess="0"
									qValues=""
								/>
							</SOAP:Body>
						</SOAP:Envelope>
					</xml>
				</xml>
			</xhead>
			<xbody>
				<xforms:group
					wcpforms:layout="vertical"
					id="xbody_group"
					wcpforms:labelalign="ontop">
					<xforms:group
						appearance="group"
						wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsGroup"
						id="group1"
						wcpforms:layout="vertical"
						wcpforms:class="f_layout"
						wcpforms:size="auto auto"
						wcpforms:anchors="5px 5px 5px 5px"
						wcpforms:position="5px 5px"
						wcpforms:labelalign="ontop"
					/>
					<xforms:group
						appearance="box"
						wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsGroupbox"
						id="groupbox1"
						isNavigationEnabled="true"
						wcpforms:layout="vertical"
						wcpforms:class="f_layout"
						wcpforms:size="auto auto"
						wcpforms:anchors="2px 2px 2px 2px"
						wcpforms:position="2px 2px"
						wcpforms:labelalign="ontop">
						<xforms:label
							textIdentifier="78e3b51c-e998-11e0-fe5b-a88997685349"
							wcpforms:class="groupheader">全部门店生命周期统计图</xforms:label>
						<xforms:div
							xformextendedname="chart"
							id="chart1"
							namespace="cas.xforms.runtimelibrary.SingleSeries"
							wcpforms:designLibrary="cas.xforms.designerlibrary.controls.extensions.SingleSeries"
							caption="全部门店生命周期统计图"
							chartType="Column 3D"
							xAxisCaption="状态"
							yAxisCaption="数量"
							showAllRecords="1"
							highlightActiveRecord="1"
							yAxisMinValue="0"
							yAxisMaxValue="10"
							wcpforms:class="f_layout"
							wcpforms:size="auto auto"
							wcpforms:anchors="5px 5px 5px 5px"
							wcpforms:position="5px 5px"
						/>
						<xforms:hint
							textIdentifier="78e3b51c-e998-11e0-fe5b-a88997685349">全部门店生命周期统计图</xforms:hint>
					</xforms:group>
				</xforms:group>
			</xbody>
			<xformsscript>
				<![CDATA[function Form_InitDone(eventObject)
{
    application.container.maximize();
    showChart1();
}

function showChart1()
{
    var getStatisticsShopByLifecycleStatusReq = cordys.cloneXMLDocument(SOAP_GetStatisticsShopByLifecycleStatus.XMLDocument);
    SOAPRequestModel.clear();
    SOAPRequestModel.setMethodRequest(getStatisticsShopByLifecycleStatusReq);
    SOAPRequestModel.reset();
    chart1.setChartData(SOAPRequestModel.getData(), true);
    chart1.setLabelColumn(".//*[local-name()='生命周期状态']");
    chart1.setValueColumn([".//*[local-name()='shop_number']"]);
    chart1.render();
}]]>
			</xformsscript>
			<xforms:action
				ev:event="xforms-initialize-done">
				<xforms:Form_InitDone/>
			</xforms:action>
