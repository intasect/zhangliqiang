			<xformsscript>
				<![CDATA[function button1_Click(eventObject)
{
    application.select(cordys.cloneXMLDocument(App_INBOX.XMLDocument).documentElement);
}

function Form_InitDone(eventObject)
{
    var getTaskCountReq = cordys.cloneXMLDocument(SOAP_GetTaskCount.XMLDocument);
    commonModel.clear();
    commonModel.setMethodRequest(getTaskCountReq );
    commonModel.reset();

    var _taskCount = cordys.getNodeText(commonModel.getData(), ".//*[local-name()='TaskCount']", "");
    taskCount.setValue(_taskCount)
   
    var userDn = system.getUser().organizations[application.organization].userDN;
    commonModel.clear();
    var req = cordys.cloneXMLDocument(SOAP_GetStatisticsTimeoutLogInfoByUser.XMLDocument);
    cordys.setNodeText(req, ".//*[local-name()='userDn']", userDn);
    commonModel.setMethodRequest(req);
    commonModel.reset();

    chart1.setChartData(commonModel.getData(),true);
    chart1.setLabelColumn(".//*[local-name()='log_month']");
    chart1.setValueColumn([".//*[local-name()='流程执行效率告警问题总数']"]);
    chart1.render();

}

function chart1_ondataclick(eventObject)
{
    application.select(cordys.cloneXMLDocument(App_INBOX.XMLDocument).documentElement);
}]]>
			</xformsscript>
			<xforms:action
				ev:event="xforms-initialize-done">
				<xforms:Form_InitDone/>
			</xforms:action>
