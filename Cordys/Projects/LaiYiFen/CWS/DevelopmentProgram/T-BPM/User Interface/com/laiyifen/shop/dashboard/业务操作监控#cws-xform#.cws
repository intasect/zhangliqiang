									</xforms:action>
								</xforms:trigger>
							</xforms:group>
							<xforms:hint
								textIdentifier="78e3b51c-e998-11e0-fe93-ea49d0f6df80">个人待办事项统计</xforms:hint>
						</xforms:group>
						<xforms:group
							appearance="box"
							wcpforms:designLibrary="cas.xforms.designerlibrary.controls.XFormsGroupbox"
							id="groupbox2"
							isNavigationEnabled="true"
							wcpforms:layout="vertical"
							wcpforms:class="v_layout_i"
							wcpforms:size="null 337px"
							wcpforms:labelalign="ontop">
							<xforms:label
								textIdentifier="78e3b51c-e998-11e0-fe94-a7bea4fb1bb3"
								wcpforms:class="groupheader">个人超时业务事项月度统计</xforms:label>
							<div
								xmlns="http://www.w3.org/2002/xforms/cr"
								xformextendedname="chart"
								namespace="cas.xforms.runtimelibrary.MultiSeries"
								wcpforms:designLibrary="cas.xforms.designerlibrary.controls.extensions.MultiSeries"
								caption="个人超时业务事项月度统计"
								chartType="Multi-series Column 3D"
								xAxisCaption="月度"
								yAxisCaption="数量"
								showAllRecords="1"
								highlightActiveRecord="1"
								yAxisMinValue="0"
								yAxisMaxValue="5"
								id="chart1"
								wcpforms:layout="vertical"
								wcpforms:class="v_layout"
								wcpforms:size="null 300px"
							/>
							<xforms:hint
								textIdentifier="78e3b51c-e998-11e0-fe94-a7bea4fb1bb3">个人超时业务事项月度统计</xforms:hint>
						</xforms:group>
