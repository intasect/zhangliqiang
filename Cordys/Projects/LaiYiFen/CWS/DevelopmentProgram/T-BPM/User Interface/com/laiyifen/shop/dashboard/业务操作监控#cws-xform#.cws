				<xml
					xmlns="">
					<xml
						id="SOAP_GetTaskCount">
						<SOAP:Envelope
							xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/">
							<SOAP:Body>
								<GetTaskCountByCriteria
									xmlns="http://schemas.cordys.com/notification/workflow/1.0">
									<TaskIdentifiers>
										<div
											xmlns="http://www.w3.org/2002/xforms/cr"
											xmlns:wcpforms="http://schemas.cordys.com/wcp/xforms"
											xformextendedname="chart"
											namespace="cas.xforms.runtimelibrary.MultiSeries"
											wcpforms:designLibrary="cas.xforms.designerlibrary.controls.extensions.MultiSeries"
											caption="个人超时业务事项月度统计"
											chartType="Multi-series Column 3D"
											xAxisCaption="月度"
											yAxisCaption="数量"
											showAllRecords="1"
											highlightActiveRecord="1"
											yAxisMinValue="0"
											yAxisMaxValue="5"
										/>
									</TaskIdentifiers>
									<Criteria>
										<Query/>
										<Parameters>
											<Parameter
												name=""
												type=""
												value=""
											/>
										</Parameters>
									</Criteria>
									<Target
										type=""
									/>
									<ShowNonWorkableItems/>
								</GetTaskCountByCriteria>
							</SOAP:Body>
						</SOAP:Envelope>
					</xml>
					<xml
						id="SOAP_GetStatisticsTimeoutLogInfoByUser">
						<SOAP:Envelope
							xmlns:SOAP="http://schemas.xmlsoap.org/soap/envelope/">
							<SOAP:Body>
								<GetStatisticsTimeoutLogInfoByUser
									xmlns="http://schemas.cordys.com/tbpm/shop"
									preserveSpace="no"
									qAccess="0"
									qValues="">
									<userDn>PARAMETER</userDn>
								</GetStatisticsTimeoutLogInfoByUser>
							</SOAP:Body>
						</SOAP:Envelope>
					</xml>
					<xml
						id="App_INBOX">
						<Application>
							<id>App_StatisticsOpenShop</id>
							<description>MyInbox</description>
							<organization>o=laiyifen,cn=cordys,cn=defaultInst,o=laiyifen</organization>
							<caption>代办列表</caption>
							<!--url>/cordys/com/cordys/notification/inbox.caf</url-->
							<url>/cordys/laiyifen/common/html/inbox/inbox.htm</url>
							<frame>main</frame>
						</Application>
					</xml>
				</xml>
